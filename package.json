{
  "name": "mock-eateries",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {

    "test": "cross-env NODE_ENV=test jest --testTimeout=50000 --forceExit",
    "pretest": "cross-env NODE_ENV=test npm run reset:db",
    "start": "babel-node server/app.js",
    "start:dev": "nodemon --exec babel-node server/app.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "seedall": "sequelize db:seed:all",
    "reset:db": "npm run migrate:undo && npm run migrate && npm run seedall",
    "heroku-postbuild": "npm run migrate",
    "coverage": "npm run pretest && jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mekzy-o/prunedge-interview.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mekzy-o/prunedge-interview/issues"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "verbose": true
  },
  "homepage": "https://github.com/mekzy-o/prunedge-interview#readme",
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-redis": "^4.0.3",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "redis": "^2.8.0",
    "regenerator-runtime": "^0.13.3",
    "sequelize": "^5.21.1",
    "sequelize-cli": "^5.5.1",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "coveralls": "^3.0.7",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "supertest": "^4.0.2"
  }
}
